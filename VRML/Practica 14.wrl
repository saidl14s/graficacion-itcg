	geometry Cylinder {
		radius .12
		height 4
	}
	appearance Appearance{
		material Material{
			emissiveColor .5 .5 .5
			diffuseColor .2 .2 .2
			specularColor .71 .70 .56
			shininess .16
		}
	}
}

